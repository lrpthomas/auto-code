{
    "name": "orchestrator-alpha",
    "version": "1.0.0",
    "description": "Elite AI agent team for autonomous app development",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "ts-node src/index.ts",
        "test": "vitest run",
        "lint": "eslint src --ext .ts",
        "typecheck": "tsc --noEmit",
        "review": "node code-review-demo.js",
        "review:quality": "ts-node -e \"import('./agents/code-quality-agent').then(agent => console.log('Quality agent loaded'))\"",
        "review:security": "ts-node -e \"import('./agents/security-review-agent').then(agent => console.log('Security agent loaded'))\"",
        "review:performance": "ts-node -e \"import('./agents/performance-review-agent').then(agent => console.log('Performance agent loaded'))\"",
        "review:architecture": "ts-node -e \"import('./agents/architecture-review-agent').then(agent => console.log('Architecture agent loaded'))\"",
        "review:documentation": "ts-node -e \"import('./agents/documentation-review-agent').then(agent => console.log('Documentation agent loaded'))\""
    },
    "keywords": [
        "ai",
        "automation",
        "app-builder",
        "orchestrator"
    ],
    "author": "ORCHESTRATOR-ALPHA Team",
    "license": "MIT",
    "dependencies": {
        "@types/node": "^20.10.0",
        "axios": "^1.6.0",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "handlebars": "^4.7.8",
        "js-yaml": "^4.1.0",
        "lodash": "^4.17.21",
        "natural": "^6.12.0",
        "openai": "^4.20.0",
        "winston": "^3.11.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/js-yaml": "^4.0.9",
        "@types/lodash": "^4.14.202",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "eslint": "^8.54.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.3.0",
        "vitest": "^3.2.4"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}